{
  "name": "telegram-emotes",
  "version": "0.1.0",
  "description": "Bot for sending emotes to Telegram.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.prod.json && npm prune --production",
    "start": "node .",
    "once": "tsc && npm start",
    "dev": "tsc-watch --onSuccess \"node . dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keanuplayz/Telegram-Emotes.git"
  },
  "keywords": [
    "telegraf",
    "telegram",
    "bot"
  ],
  "author": "Keanu Timmermans",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/keanuplayz/Telegram-Emotes/issues"
  },
  "homepage": "https://github.com/keanuplayz/Telegram-Emotes#readme",
  "dependencies": {
    "telegraf": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/ws": "^7.4.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.4"
  }
}
